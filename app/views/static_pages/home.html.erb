<% provide :title, t(".name_page") %>

<div class="center jumbotron">
  <h1><%= t ".title", title: t(".title") %></h1>
  <p><%= raw t ".description",href: link_to(t(".title_link"),
   "https://www.railstutorial.org/") %></p>
  <%= link_to t(".sign_up_now"), signup_path, class: "btn btn-lg btn-primary" %>
</div>
<%= link_to image_tag("railsIcon.jpg",
  alt: "Rails logo", width: "200px"),"https://rubyonrails.org/" %>
